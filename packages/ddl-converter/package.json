{
  "name": "@biagram/ddl-converter",
  "version": "0.1.0",
  "private": true,
  "description": "DDL to DBML converter supporting MySQL and PostgreSQL",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@biagram/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
