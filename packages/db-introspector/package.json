{
  "name": "@biagram/db-introspector",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./mysql": {
      "types": "./dist/mysql.d.ts",
      "import": "./dist/mysql.js",
      "require": "./dist/mysql.cjs"
    },
    "./postgresql": {
      "types": "./dist/postgresql.d.ts",
      "import": "./dist/postgresql.js",
      "require": "./dist/postgresql.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts src/mysql.ts src/postgresql.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/mysql.ts src/postgresql.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@biagram/shared": "workspace:*",
    "mysql2": "^3.6.5",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biagram/eslint-config": "workspace:*",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
